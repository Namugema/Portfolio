@page "{projectId:int}"

@model Portfolio.Pages.Projects.DetailModel
@{
    ViewData["Title"] = "Detail";
}

<h3>@Model.Project.Name</h3>
<div>
    Id: @Model.Project.Id
</div>
<div>
    Description: @Model.Project.Description
</div>
<div>
    Status: @Model.Project.Status
</div>

<a asp-page="./List" class="btn btn-default">All Projects</a>
